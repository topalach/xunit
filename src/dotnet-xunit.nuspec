<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2013/05/nuspec.xsd">
  <metadata>
    <id>dotnet-xunit</id>
    <version>2.4.0-beta.1.build10001</version>
    <title>xUnit.net [Runner: .NET CLI SDK]</title>
    <authors>James Newkirk,Brad Wilson</authors>
    <summary>.NET CLI SDK runner for the xUnit.net framework.</summary>
    <description>.NET CLI SDK runner for the xUnit.net framework. Supports .NET 4.5.2 or later, .NET Core 1.x, and .NET Core 2.x.</description>
    <language>en-US</language>
    <projectUrl>https://github.com/xunit/xunit</projectUrl>
    <iconUrl>https://raw.githubusercontent.com/xunit/media/master/logo-512-transparent.png</iconUrl>
    <licenseUrl>https://raw.githubusercontent.com/xunit/xunit/master/license.txt</licenseUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <packageTypes>
      <packageType name="DotnetCliTool" />
    </packageTypes>
  </metadata>
  <files>
    <file src="dotnet-xunit\prefercliruntime" target="prefercliruntime" />

    <file src="dotnet-xunit\bin\$Configuration$\netcoreapp2.0\dotnet-xunit.dll"                target="lib\netcoreapp2.0\" />
    <file src="dotnet-xunit\bin\$Configuration$\netcoreapp2.0\dotnet-xunit.runtimeconfig.json" target="lib\netcoreapp2.0\" />
    <file src="dotnet-xunit\bin\$Configuration$\netcoreapp2.0\import.targets"                  target="lib\netcoreapp2.0\" />

    <file src="xunit.console\bin\$Configuration$\net452\win7-x86\xunit.abstractions.dll"            target="tools\net452\" />
    <file src="xunit.console\bin\$Configuration$\net452\win7-x86\xunit.console.exe"                 target="tools\net452\" />
    <file src="xunit.console\bin\$Configuration$\net452\win7-x86\xunit.console.exe.config"          target="tools\net452\" />
    <file src="xunit.console\bin\$Configuration$_x86\net452\win7-x86\xunit.console.x86.exe"         target="tools\net452\" />
    <file src="xunit.console\bin\$Configuration$_x86\net452\win7-x86\xunit.console.x86.exe.config"  target="tools\net452\" />
    <file src="xunit.console\bin\$Configuration$\net452\win7-x86\xunit.runner.reporters.net452.dll" target="tools\net452\" />
    <file src="xunit.console\bin\$Configuration$\net452\win7-x86\xunit.runner.utility.net452.dll"   target="tools\net452\" />

    <file src="xunit.console\bin\$Configuration$\netcoreapp2.0\publish\*.*" target="tools\netcoreapp2.0\" />
  </files>
</package>
